{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": { "distDir": "dist" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html",
            "has": [
                {
                    "type": "missing",
                    "value": "/.*\\.(js|css|png|jpg|jpeg|svg|gif|ico|json|txt|woff2?)$"
                }
            ]
        }
    ]
}